{
  "name": "@microsoft/a2achat-core",
  "version": "0.1.0",
  "description": "TypeScript SDK for browser user interaction with AI Agents through the A2A protocol",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./chat": {
      "types": "./dist/chat/index.d.ts",
      "import": "./dist/chat/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js"
    },
    "./svelte": {
      "types": "./dist/svelte/index.d.ts",
      "import": "./dist/svelte/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "check": "npm run typecheck && npm run test -- --run && npm run build",
    "prepublishOnly": "npm run check"
  },
  "keywords": [
    "a2a",
    "a2a-protocol",
    "ai",
    "agents",
    "ai-agents",
    "sdk",
    "browser",
    "typescript",
    "react",
    "chat",
    "streaming"
  ],
  "author": "Travis Vu",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/events": "^3.0.3",
    "@types/node": "^24.0.12",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0",
    "svelte": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "p-retry": "^6.2.1",
    "zod": "^4.0.2"
  },
  "sideEffects": false
}
