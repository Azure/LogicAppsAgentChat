{
  "name": "@microsoft/a2achat",
  "version": "0.1.0",
  "description": "Lightweight, customizable chat interface library",
  "type": "module",
  "main": "./dist/lib/index.cjs",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.cjs"
    },
    "./styles.css": "./dist/lib/styles.css"
  },
  "files": [
    "dist/lib"
  ],
  "scripts": {
    "dev": "vite",
    "demo": "vite --host",
    "build:lib": "vite build --mode lib",
    "build:iframe": "vite build --mode iframe",
    "build:demo": "vite build --mode demo",
    "build": "npm run build:lib && npm run build:iframe && npm run build:demo",
    "build:all": "npm run build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "preview": "vite preview",
    "preview:demo": "vite preview --outDir dist/demo",
    "serve:demo": "node serve-demo.js",
    "serve:static": "npx serve dist/demo",
    "size": "size-limit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@types/prismjs": "^1.26.5",
    "a2a-browser-sdk": "^0.1.0",
    "marked": "^12.0.0",
    "marked-highlight": "^2.2.1",
    "prismjs": "^1.30.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.56.0",
    "happy-dom": "^18.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.0.2",
    "terser": "^5.26.0",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "size-limit": [
    {
      "path": "dist/lib/index.js",
      "limit": "45 KB"
    }
  ],
  "sideEffects": false,
  "keywords": [
    "chat",
    "widget",
    "react",
    "lightweight",
    "customizable"
  ],
  "author": "",
  "license": "MIT"
}
